{
  "name": "agent-chain-sdk",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript/JavaScript library for the AgentChain platform - blockchain-powered AI agents with tokenized economies on Solana",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc",
    "prepublishOnly": "npm run build && npm test",
    "postversion": "git push && git push --tags",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "agentchain",
    "solana",
    "ai",
    "blockchain",
    "crypto",
    "agents",
    "defi",
    "web3",
    "pumpfun",
    "tokens",
    "typescript",
    "sdk"
  ],
  "author": "AgentChain Team",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "@supabase/supabase-js": "^2.38.5",
    "moralis": "^2.25.0",
    "openai": "^4.20.1",
    "ws": "^8.14.2",
    "axios": "^1.6.2",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/jest": "^29.5.8",
    "@types/ws": "^8.5.9",
    "@types/crypto-js": "^4.2.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentchain/agent-chain-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/agentchain/agent-chain-sdk/issues"
  },
  "homepage": "https://github.com/agentchain/agent-chain-sdk#readme"
}
